class Solution {
    Integer[] dp;
    public int rob(int[] nums) {
        int n=nums.length;
        dp=new Integer[n];
        return solve(nums,n-1);
    }
    public int solve(int[] nums,int i){
        if(i<0) return 0;
        if(dp[i]!=null) return dp[i];
        int pic=nums[i]+solve(nums,i-2);
        int notPic=solve(nums,i-1);
        return dp[i]=Math.max(pic,notPic);
    }
}
