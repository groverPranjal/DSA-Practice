class Solution {
    public int evalRPN(String[] tokens) {
       Stack<Integer> st=new Stack<>();
       for(String s:tokens){
        if(s.equals("+")||s.equals("-")||s.equals("*")||s.equals("/") ){
            int a=st.pop();
            int b=st.pop();
            int ans=op(a,b,s);
            st.push(ans);
        }
        else st.push(Integer.parseInt(s));
       } 
       return st.pop();
    }
    public int op(int b,int a,String s){
        if(s.equals("+")) return a+b;
        if(s.equals("-")) return a-b;
        if(s.equals("*")) return a*b;
        if(s.equals("/")) return a/b;
        return -1;
    }
    
}
