Brute Force: TLE TC=o(n^2);
class Solution {
    public int[] productExceptSelf(int[] nums) {
        int n=nums.length;
        int[] res=new int[n];
        for(int i=0;i<n;i++){
            int pro=1;
            for(int j=0;j<n;j++){
               if(i!=j) pro*=nums[j];
            }
            res[i]=pro;
        }
        return res;
    }
}
Approch-2 using Division
class Solution {
    public int[] productExceptSelf(int[] nums) {
        int count=0;
        int prod_without_zero=1;
        for(int i:nums){
            if(i==0) count++;
            else prod_without_zero*=i;
        }
        int[] res=new int[nums.length];
        for(int i=0;i<nums.length;i++){
            if(nums[i]!=0){
                if(count>0) res[i]=0;
                else res[i]=prod_without_zero/nums[i];
            }
            else{
                if(count>1) res[i]=0;
                else res[i]=prod_without_zero;
            }
        }
        return res;
    }
}
Approch-3 using prefix suffix 
class Solution {
    public int[] productExceptSelf(int[] nums) {
        int n=nums.length;
        int[] left=new int[n];
        int[] right=new int[n];
        int[] res=new int[n];
        left[0]=1;
        for(int i=1;i<n;i++)  left[i]=nums[i-1]*left[i-1];
        right[n-1]=1;
        for(int i=n-2;i>=0;i--) right[i]=nums[i+1]*right[i+1];
        for(int i=0;i<n;i++) res[i]=left[i]*right[i];
        return res;
    }
}
Approch 4- No extra space
class Solution {
    public int[] productExceptSelf(int[] nums) {
        int[] res=new int[nums.length];
        int n=nums.length;
        res[0]=1;
        for(int i=1;i<n;i++) {
            res[i]=res[i-1]*nums[i-1];
        }
        int right=1;
        for(int i=n-1;i>=0;i--){
            res[i]=res[i]*right;
            right*=nums[i];
        }
        return res;
    }
}
