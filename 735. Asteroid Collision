class Solution {
    public int[] asteroidCollision(int[] ast) {
        Stack<Integer> s=new Stack<>();
        for(int i:ast){

            while(!s.isEmpty() && i<0 && s.peek()>0){
                int sum=i+s.peek();
                if(sum<0) s.pop();
                else if(sum>0) {
                    i=0;
                    break;
                }
                else{
                    s.pop();
                    i=0;
                    break;
                }
            }
            if(i!=0) s.push(i);
        }
        int[] ans=new int[s.size()];
        int n=s.size();
        for(int i=n-1;i>=0;i--){
            int num=s.pop();
            ans[i]=num;
        }
        return ans;
    }
}
