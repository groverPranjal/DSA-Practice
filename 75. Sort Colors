Approch 1:
class Solution {
    public void sortColors(int[] nums) {
        int[] freq=new int[3];
        for(int i=0;i<nums.length;i++){
            if(nums[i]==0) freq[0]++;
            if(nums[i]==1) freq[1]++;
            if(nums[i]==2) freq[2]++;
        }
        int idx=0;
        for(int i=0;i<freq[0];i++) nums[idx++]=0;
        for(int i=0;i<freq[1];i++) nums[idx++]=1;
        for(int i=0;i<freq[2];i++) nums[idx++]=2;

    }
}
Approch 2:
class Solution {
    public void sortColors(int[] nums) {
        int low=0;
        int mid=0;
        int high=nums.length-1;
        while(mid<=high){
            if(nums[mid]==0){
                swap(nums,low,mid);
                low++;
                mid++;
            }
            else if(nums[mid]==1) mid++;
            else {
                swap(nums,mid,high);
                high--;
                }
        }
    }
    public void swap(int[] nums,int i,int j){
        int temp=nums[i];
        nums[i]=nums[j];
        nums[j]=temp;
    }
}
