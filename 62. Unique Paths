class Solution {
    Integer[][] dp;
    public int uniquePaths(int m, int n) {
        dp=new Integer[m][n];
        return solve(m-1,n-1);
    }
    public int solve(int i,int j){
        if(i==0 && j==0) return 1;
        if(i<0 || j<0) return 0;
        if(dp[i][j]!=null) return dp[i][j];
        return dp[i][j]=solve(i-1,j)+solve(i,j-1);
    }
}
